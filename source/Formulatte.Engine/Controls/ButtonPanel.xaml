<UserControl x:Class="Formulatte.Engine.Controls.ButtonPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ui="http://schemas.inkore.net/lib/ui/wpf/modern"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Border Background="{DynamicResource {x:Static ui:ThemeKeys.CommandBarFlyoutBackgroundKey}}" CornerRadius="8"
            BorderBrush="{DynamicResource {x:Static ui:ThemeKeys.CardStrokeColorDefaultBrushKey}}" BorderThickness="1"
            Padding="5">
        <UniformGrid x:Name="mainGrid">
        </UniformGrid>
    </Border>
</UserControl>
